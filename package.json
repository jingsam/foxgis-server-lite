{
  "name": "foxgis-server-lite",
  "version": "2.0.0",
  "bin": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon",
    "docs": "docsify serve ./docs --open",
    "test": "tape test/**/*.js",
    "lint": "eslint app/**/*.js",
    "format": "prettier-eslint --write app/**/*.js",
    "dist": "pkg . --out-path=dist/ --targets=win,linux,macos"
  },
  "engines": {
    "node": ">=10"
  },
  "pkg": {
    "assets": [
      "node_modules/@mapbox/glyph-pbf-composite/proto/glyphs.proto"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{app,test}/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "@mapbox/glyph-pbf-composite": "^0.0.3",
    "@mapbox/mbtiles": "^0.12.1",
    "compression": "^1.6.2",
    "consolidate": "^0.16.0",
    "cors": "^2.8.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "docsify-cli": "^4.4.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "pkg": "^5.1.0",
    "prettier": "^2.3.0",
    "prettier-eslint-cli": "^5.0.1",
    "supertest": "^6.1.3",
    "tape": "^5.2.2"
  }
}
